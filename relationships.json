{
    "matches": {
        "from_types": ["Domain", "Url", "Text"],
        "to_types": ["DatasetEntry"],
        "templates": {
            "en": "according to {to.sources} is {to.label}"
        }
    },
    "belongs_to_domain": {
        "from_types": ["Url"],
        "to_types": ["Domain"],
        "templates": {
            "en": "belongs to domain {to.domain}"
        }
    },
    "contains_url": {
        "from_types": ["Tweet"],
        "to_types": ["Url"],
        "templates": {
            "en": "contains {to.url}"
        }
    },
    "contains_text": {
        "from_types": ["Tweet"],
        "to_types": ["Text"],
        "templates": {
            "en": "contains {to.text}"
        }
    },
    "writes": {
        "from_types": ["User"],
        "to_types": ["Tweet"],
        "templates": {
            "en": "tweeted {to.text}"
        }
    },
    "follows": {
        "from_types": ["User"],
        "to_types": ["User"],
        "templates": {
            "en": "follows {to.screen_name}"
        }
    }
}